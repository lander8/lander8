<div class="container-fluid" style="margin-top: 100px">
	<div class="row">
		<div class="container">
			<div class="col-xs-6">
				<% current_user.websites.each do |website| %>
					<h3><%= website.name %></h3>
					<p><%= website.url %></p>
					<p><%= website.api_key %></p>
				<% end %>
			</div>
			<div class="col-xs-6">
				<a href="/websites/new">Add a Website</a>
			</div>
		</div>
	</div>
</div>